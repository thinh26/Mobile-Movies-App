{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    },
    "module": "ESNext",
    "target": "ESNext",
    "types": ["@stylistic/eslint-plugin"],
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "namespaces/environment.d.ts",
    "eslint.config.mjs"
  ],
  "exclude": ["app-example"]
}
